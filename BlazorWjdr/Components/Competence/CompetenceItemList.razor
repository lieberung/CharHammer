@using BlazorWjdr.Models
<ul class="list-inline">
    @foreach (var item in Items)
    {
        <li class="list-inline-item"><CompetenceItem Item="@item"/></li>
    }
    @foreach (var choix in ItemsChoix)
    {
        <li class="list-inline-item"><ChoixDeCompetenceItem Choix="@choix"/></li>
    }
</ul>
@code{
    [Parameter] public CompetenceDto[] Items { get; set; } = null!;
    [Parameter] public CompetenceDto[][] ItemsChoix { get; set; } = System.Array.Empty<CompetenceDto[]>();
}