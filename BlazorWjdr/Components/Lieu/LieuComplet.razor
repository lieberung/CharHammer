@using BlazorWjdr.Models

<ul class="list-inline">
    @if (Item.Parent is {Ignorer: false })
    {
        @if (Item.Parent.Parent is {Ignorer: false })
        {
            <li class="list-inline-item">
                <LieuItem Item="@Item.Parent.Parent"/>
            </li>
        }
        <li class="list-inline-item">
            <LieuItem Item="@Item.Parent"/>
        </li>
    }
    <li class="list-inline-item">
        <LieuItem Item="@Item"/>
    </li>
</ul>

@code {
    [Parameter]
    public LieuDto Item { get; set; } = null!;
}