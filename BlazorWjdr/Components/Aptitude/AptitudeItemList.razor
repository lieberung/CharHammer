@using BlazorWjdr.Models

<ul class="list-inline">
    @foreach (var item in Items)
    {
        <li class="list-inline-item"><AptitudeItem Item="@item"/></li>
    }
    @foreach (var choix in ItemsChoix)
    {
        <li class="list-inline-item"><ChoixDAptitudeItem Choix="@choix"/></li>
    }
</ul>

@code{
    [Parameter] public AptitudeDto[] Items { get; set; } = null!;
    [Parameter] public AptitudeDto[][] ItemsChoix { get; set; } = Array.Empty<AptitudeDto[]>();
}